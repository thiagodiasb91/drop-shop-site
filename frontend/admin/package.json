{
  "name": "admin-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build && vite preview --port 5173",
    "deploy:s3": "aws s3 sync dist/ s3://drop-shop-admin-frontend --delete --cache-control 'max-age=31536000,public'",
    "deploy:cdn": "aws cloudfront create-invalidation --distribution-id E2A7MA05O5ZR0P --paths /index.html",
    "deploy": "run-s build deploy:s3 deploy:cdn"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.24",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.15.8",
    "@tailwindcss/vite": "^4.1.18"
  }
}